package lecture_1_basics;

import java.util.Scanner;

public class reverse_mum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner jk = new Scanner(System.in);
		int n = jk.nextInt();
		int ans = 0;
		while(n!=0) {
			int rem = n%10;
			ans = ans*10 + rem;
			n = n/10;
			
		}
		System.out.println(ans);
	
	}

}
